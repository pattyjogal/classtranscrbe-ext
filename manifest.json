{
    "manifest_version": 2,
    "name": "ClassTranscribe Extensions",
    "version": "0.2.0",

    "description": "Adds some convenient functionality to ClassTranscribe",

    "icons": {
        "48": "assets/img/icon.png"
    },

    "permissions": ["storage"],

    "content_scripts": [
        {
            "matches": ["*://*.classtranscribe.illinois.edu/watchLectureVideos/*"],
            "js": ["assets/js/jquery-3.3.1.slim.min.js", "src/ct_extensions.js"]
        }
    ]

}
